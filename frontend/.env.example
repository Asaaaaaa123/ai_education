# 环境配置示例文件
# 复制此文件为 .env 并修改相应配置

# 应用端口
PORT=3000

# API基础URL
REACT_APP_API_URL=http://localhost:8001

# 应用环境
NODE_ENV=development

# 是否启用性能监控
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# 是否启用调试模式
REACT_APP_DEBUG_MODE=true

# 缓存配置
REACT_APP_CACHE_ENABLED=true
REACT_APP_CACHE_MAX_AGE=300000

# 请求超时时间（毫秒）
REACT_APP_REQUEST_TIMEOUT=10000

# 是否启用错误报告
REACT_APP_ENABLE_ERROR_REPORTING=false

# 错误报告服务URL（如果启用）
REACT_APP_ERROR_REPORTING_URL=

# 是否启用分析
REACT_APP_ENABLE_ANALYTICS=false

# 分析服务ID（如果启用）
REACT_APP_ANALYTICS_ID=
